<h3 class="mt-4 mb-5">Welcome {{name}}</h3>
<div class="col mt-3">
  <div class="row mt-3 mb-5">
    <h5> I am requested:</h5>
    {{#each foreignRequests}}
      <span> Requested from: {{this.searcher.name}} </span>
      <span> Status: {{this.status}} </span>
    {{/each}}
  </div>

  <div class="row mt-3 mb-3">
    <h5> My requests: </h5>
    {{#each myRequests}}
      <span> Request to: {{this.requested.name}} </span>
      <span> Request for: {{this.requested_talent}} </span>
      <span> Status: {{this.status}} </span>
    {{/each}}
  </div>
</div>
